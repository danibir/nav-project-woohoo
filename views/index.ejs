<%- include('./partials/head.ejs') %>

    <%- include('./partials/navigation.ejs') %>

        <section class="indexPage">
            <div class="indexLogo">
                <img src="/img/nav-logo.png" alt="nav logo">
            </div>

            <div class="searchField">
                <label for="search"><h1>SÃ¸k:</h1></label>
                <%- include('./partials/searchfield.ejs') %>
            </div>

            <div class="container-results">
                <h2>Resultat:</h2>
                <% if (data && data.length> 0) { %>
                    <ul>
                        <% data.forEach(item=> { %>
                            <li><a href="<%= item.url %>">
                                <h3>
                                    <%= (item.title.object.nb && item.title.object.nb[0]) || (item.title.object.en && item.title.object.en[0]) %>
                                </h3>
                            </a></li>
                        <% }); %>
                    </ul>
                    <% } else { %>
                        <p>Ingen resultater funnet</p>
                        <% } %>
            </div>
        </section>



        <%- include('./partials/foot.ejs') %>